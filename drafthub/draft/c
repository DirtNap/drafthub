query {
  viewer {
    login
  }
  rateLimit {
    limit
    cost
    remaining
  }
  repository(owner: "warwick965", name: "competitive-programming"){
    object(expression: "master"){
      ... on Commit {
        history(path: "README.md", first:1){
          edges {
            node {
              pushedDate
              message
              url
              author {
                user {
                  name
                  url
                  login
                  isViewer
                }
              }
            }
          }
        }
      }
    }
  }
}

